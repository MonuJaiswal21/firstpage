{
    "manifest_version":3,
    "name":"my First Extension",
    "description":"my First extension is for injecting script inside google.com",
    "version":"1.0.0",
    "web_accessible_resoureces": [{
        "resources": ["images/logo.png"],
        "matches": ["https://*.google.com/*"]
    }],
    "content_scripts": [
        {
            "matches": ["https://*.google.com/*"],
            "run_at": "document_end",
            "js": ["content-script.js"]
        }
    ]
}